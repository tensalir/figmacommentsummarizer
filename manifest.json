{
  "name": "Figma Comment Analyzer",
  "id": "figma-comment-analyzer",
  "api": "1.0.0",
  "main": "code.js",
  "ui": "ui.html",
  "editorType": ["figma", "figjam", "dev"],
  "networkAccess": {
    "allowedDomains": [
      "https://api.anthropic.com",
      "https://api.figma.com"
    ],
    "devAllowedDomains": [
      "http://localhost:3000"
    ]
  },
  "permissions": [
    "currentuser"
  ],
  "capabilities": [
    "inspect"
  ],
  "documentAccess": "dynamic",
  "relaunchButtons": [
    {
      "command": "analyze",
      "name": "Analyze Comments",
      "multipleSelection": false
    }
  ]
} 